class Point
{
    x : int;
    y : int;
    z : int;
    
    constructor Point(_x : int, _y : int, _z : int)
    {
        x = _x;
        y = _y;
        z = _z;
    }
}

def foo : int[](p : Point)
{
    a = { p.x, p.y, p.z };
    return = a;
}

p = Point.Point(3, 4, 5);
a = foo(p);

